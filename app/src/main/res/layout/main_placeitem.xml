<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/item_layout1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <TextView
            android:id="@+id/item1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/item2"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center"
            android:background="@drawable/main_home_item_radius" />

        <TextView
            android:id="@+id/item2"
            app:layout_constraintLeft_toRightOf="@+id/item1"
            app:layout_constraintRight_toLeftOf="@+id/item3"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center"
            android:background="@drawable/main_home_item_radius" />

        <TextView
            android:id="@+id/item3"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/item2"
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center"
            android:background="@drawable/main_home_item_radius" />

        <TextView
            android:id="@+id/item4"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/item5"
            app:layout_constraintTop_toBottomOf="@+id/item1"
            android:layout_marginTop="10dp"
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center"
            android:background="@drawable/main_home_item_radius" />

        <TextView
            android:id="@+id/item5"
            app:layout_constraintLeft_toRightOf="@+id/item4"
            app:layout_constraintRight_toLeftOf="@+id/item6"
            app:layout_constraintTop_toBottomOf="@+id/item2"
            android:layout_marginTop="10dp"
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center"
            android:background="@drawable/main_home_item_radius" />

        <TextView
            android:id="@+id/item6"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/item3"
            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toRightOf="@+id/item5"
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center"
            android:background="@drawable/main_home_item_radius" />



        <TextView
            android:id="@+id/item7"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/item8"
            app:layout_constraintTop_toBottomOf="@+id/item5"
            android:layout_marginTop="10dp"
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center"
            android:background="@drawable/main_home_item_radius" />

        <TextView
            android:id="@+id/item8"
            app:layout_constraintLeft_toRightOf="@+id/item7"
            app:layout_constraintRight_toLeftOf="@+id/item9"
            app:layout_constraintTop_toBottomOf="@+id/item6"
            android:layout_marginTop="10dp"
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center"
            android:background="@drawable/main_home_item_radius" />

        <TextView
            android:id="@+id/item9"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/item6"
            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toRightOf="@+id/item8"
            android:layout_width="110dp"
            android:layout_height="40dp"
            android:text=""
            android:gravity="center"
            android:background="@drawable/main_home_item_radius" />

    </androidx.constraintlayout.widget.ConstraintLayout>




</androidx.constraintlayout.widget.ConstraintLayout>